  - question: "\u72D7\u5403\u4E86\u4EC0\u4E48\uFF1F"
    answer: "\u72D7\u88AB\u5F15\u8BF1\u53BB\u5403\u5C38\u4F53\u3002"
  - question: "\u72D7\u4E3A\u4EC0\u4E48\u5E26\u56DE\u9AA8\u5934\uFF1F"
